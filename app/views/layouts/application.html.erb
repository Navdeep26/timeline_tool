    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <style type="text/css">
      .signout{
          color: white;
          text-decoration: none;
          /* padding: 2.2rem 0rem; */
          padding: 2.2rem;
          vertical-align: middle;
          display: inline-block;
          float: right;
      }
    </style>
    <% if !params[:task_id].present? && !params[:name].present? %>
        <div class="header sticky" >
            <div class="row fixed-width" >
                <div class="col-xs-5 col-sm-6 col-md-6" >
                    <h2> <a target="" style="text-decoration: none;color: white;" href="/">Timeline Tool</a> </h1>
                </div>
                    <div class="col-xs-7 col-sm-6 col-md-6" >
                        <a class="show_graph" target="" style="float: right; color: white; text-decoration: none; padding: 2.2rem 0rem;" href="/show_graph">
                            <span style="font-size: 1.7rem;" class="glyphicon glyphicon-stats" ></span> Show Graph
                        </a>
                        <!-- <a style="float: right; color: white; text-decoration: none; padding: 2.2rem 0rem;" href="/sign-out"> <span class="glyphicon glyphicon-user" >Sign out</span> </a> -->
                        <a class="create_task none" target="" style="float: right;color: white; text-decoration: none; padding: 2.2rem 0rem;" href="/">
                            <span style="font-size: 1.7rem;" class="glyphicon glyphicon-tasks" ></span> Create Task
                        </a>
                        <a class="signout" target="" style="color: white; text-decoration: none;" href="/sign-out">
                            <span style="font-size: 1.7rem;" class="glyphicon glyphicon-user" ></span> Sign Out
                        </a>
                        <!-- <a style="float: right; color: white; text-decoration: none; padding: 2.2rem 0rem;" href="/sign-out"> <span class="glyphicon glyphicon-user"> Sign out</span> </a> -->
                    </div>
            </div>
        </div>
    <% end %>
    <div class="" >
        <%= yield %>
    </div>

    <script src="https://code.jquery.com/jquery-3.3.1.min.js"></script>

    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>
    
    <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.8.0/js/bootstrap-datepicker.min.js"></script>
  </body>
</html>
